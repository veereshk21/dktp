{
  "name": "one-digital-desktop",
  "version": "1.0.0",
  "description": "1D - Digital First Desktop",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "author": "Â© 2017 Verizon Wireless",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run clean",
    "bundle:prod": "npm run bundle:clean && cross-env NODE_ENV=production node ./internals/webpack/bundle_page.js -b all",
    "bundle:prod:page": "cross-env NODE_ENV=production node ./internals/webpack/bundle_page.js -b",
    "bundle:dev": "npm run bundle:clean && cross-env NODE_ENV=development node ./internals/webpack/bundle_page.js -b all",
    "bundle:dev:page": "cross-env NODE_ENV=development node ./internals/webpack/bundle_page.js -b",
    "check:pages": "node ./internals/webpack/bundle_page.js -p",
    "bundle:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=node_development node server",
    "presetup": "npm i chalk shelljs",
    "setup": "npm run presetup && npm install",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .eslintignore --ignore-pattern internals/**",
    "lint:js": "npm run lint:eslint -- . --fix",
    "precommit": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build:page": "node ./internals/webpack/bundle_page.js -h"
  },
  "fontsPath": "app/fonts",
  "imagesPath": "app/images",
  "lint-staged": {
    "*.js": [
      "eslint --ignore-path .eslintignore --ignore-pattern internals/**",
      "git add"
    ]
  },
  "pre-commit": "precommit",
  "babel": {
    "presets": [
      [
        "env",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react",
      "stage-1"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "redux-saga",
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "no-plusplus": [
        2,
        {
          "allowForLoopAfterthoughts": true
        }
      ],
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "redux-saga/no-yield-in-race": 2,
      "redux-saga/yield-effects": 2,
      "require-yield": 0,
      "import/no-webpack-loader-syntax": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.prod.babel.js"
        }
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}"
    ],
    "globals": {
      "__webpack_public_path__": 0
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "dependencies": {
    "axios": "0.17.1",
    "babel-polyfill": "6.26.0",
    "bluebird": "3.5.0",
    "classlist-polyfill": "1.2.0",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "express": "4.14.0",
    "focus-trap-react": "^3.1.2",
    "fontfaceobserver": "2.0.13",
    "history": "^4.7.2",
    "html-react-parser": "0.4.1",
    "immutable": "3.8.2",
    "invariant": "2.2.2",
    "ip": "1.1.4",
    "lodash": "4.17.4",
    "material-ui": "0.20.0",
    "moment": "^2.20.1",
    "no-scroll": "^2.1.0",
    "prop-types": "15.6.1",
    "qs": "6.5.1",
    "react": "16.2.0",
    "react-a11y": "1.0.0",
    "react-accessible-accordion": "^2.2.1",
    "react-addons-css-transition-group": "15.6.2",
    "react-aria-modal": "2.12.1",
    "react-burger-menu": "2.3.0",
    "react-displace": "^2.3.0",
    "react-dom": "16.2.0",
    "react-flexbox-grid": "2.0.0",
    "react-helmet": "3.2.2",
    "react-id-swiper": "1.6.1",
    "react-lazyload": "2.3.0",
    "react-loadable": "5.3.1",
    "react-modal": "3.3.2",
    "react-redux": "5.0.7",
    "react-render-html": "0.1.6",
    "react-required-if": "1.0.1",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-sanfona": "^1.1.0",
    "react-stickynode": "^1.4.1",
    "react-tap-event-plugin": "3.0.2",
    "redux": "3.7.2",
    "redux-form": "7.2.3",
    "redux-immutable": "4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1",
    "scriptjs": "2.5.8",
    "styled-components": "2.4.0",
    "warning": "3.0.0",
    "worker-farm": "1.4.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.3",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-istanbul": "4.1.5",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.16.0",
    "babel-preset-stage-1": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "case-sensitive-paths-webpack-plugin": "2.0.0",
    "chalk": "^1.1.3",
    "cheerio": "1.0.0-rc.2",
    "circular-dependency-plugin": "2.0.0",
    "commander": "2.9.0",
    "copy-webpack-plugin": "4.0.1",
    "coveralls": "2.11.15",
    "css-loader": "0.28.10",
    "enzyme": "2.6.0",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-prefer-object-spread": "1.1.0",
    "eslint-plugin-react": "6.7.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.30.1",
    "imports-loader": "0.6.5",
    "jest-cli": "18.0.0",
    "json-loader": "0.5.4",
    "lint-staged": "3.2.1",
    "minimist": "1.2.0",
    "node-sass": "4.7.2",
    "null-loader": "0.1.1",
    "offline-plugin": "4.5.2",
    "optimize-css-assets-webpack-plugin": "1.3.0",
    "plop": "1.7.3",
    "pre-commit": "1.1.3",
    "psi": "3.0.0",
    "react-addons-test-utils": "15.4.1",
    "react-test-renderer": "15.4.2",
    "rimraf": "2.5.4",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.8",
    "sinon": "2.0.0-pre",
    "style-loader": "0.20.2",
    "url-loader": "0.5.7",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "^2.11.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.21.0"
  }
}
